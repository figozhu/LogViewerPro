{
  "name": "logviewerpro",
  "version": "1.0.3",
  "description": "跨平台日志可视化工具",
  "type": "module",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "npm-run-all build build:worker --parallel dev:worker dev:app",
    "dev:app": "electron-vite dev",
    "dev:worker": "tsc -p tsconfig.worker.json --watch",
    "build": "npm run typecheck && npm run build:worker && electron-vite build",
    "build:worker": "tsc -p tsconfig.worker.json",
    "preview": "electron-vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .ts,.vue",
    "format": "prettier --write \"src/**/*.{ts,vue,css}\"",
    "prepare": "husky",
    "postinstall": "patch-package",
    "dist": "npm run build && electron-builder -c electron-builder.config.cjs",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figozhu/LogViewerPro.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/figozhu/LogViewerPro/issues"
  },
  "homepage": "https://github.com/figozhu/LogViewerPro#readme",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "electron-store": "^11.0.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-i18n": "^9.14.5",
    "vue-virtual-scroller": "^2.0.0-beta.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.3"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,json}": "prettier --write"
  }
}
